{
  "name": "RedmineTimeTracker",
  "version": "0.8.0",
  "description": "Time Tracking tool for Redmine",
  "scripts": {
    "test": "karma start --single-run",
    "start": "electron ./app",
    "clean": "rm -rf ./release/electron",
    "pack:osx": "electron-packager ./dist RedmineTimeTracker --out=release/electron/ --platform=darwin --arch=x64 --icon=assets/osx/icon.icns --overwrite",
    "pack:win32": "electron-packager ./dist RedmineTimeTracker --out=release/electron/ --platform=win32 --arch=ia32 --icon=assets/win/icon.ico --overwrite",
    "pack:win64": "electron-packager ./dist RedmineTimeTracker --out=release/electron/ --platform=win32 --arch=x64 --icon=assets/win/icon.ico --overwrite",
    "build:osx": "npm run pack:osx && electron-builder release/electron/RedmineTimeTracker-darwin-x64/RedmineTimeTracker.app --platform=osx --out=release/electron/RedmineTimeTracker-darwin-x64-setup --config=build-config.json",
    "build:win32": "npm run pack:win32 && gulp nsi-template && electron-builder release/electron/RedmineTimeTracker-win32-ia32 --platform=win --out=release/electron/RedmineTimeTracker-win32-ia32-setup --config=build-config.json",
    "build:win64": "npm run pack:win64 && gulp nsi-template && electron-builder release/electron/RedmineTimeTracker-win32-x64 --platform=win --out=release/electron/RedmineTimeTracker-win32-x64-setup --config=build-config.json"
  },
  "postinstall": "install-app-deps",
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ujiro99/RedmineTimeTracker.git"
  },
  "keywords": [
    "redmine",
    "electron-app",
    "chrome-app"
  ],
  "devDependencies": {
    "angular-mocks": "^1.4.1",
    "chai": "~1.8.1",
    "devtron": "^1.1.0",
    "electron": "^1.3.3",
    "electron-packager": "^8.0.0",
    "electron-builder": "^2.1.1",
    "electron-prebuilt": "^0.34.3",
    "gulp": "^3.9.0",
    "gulp-replace": "^0.5.4",
    "grunt": "~0.4.1",
    "grunt-bower-task": "^0.4.0",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-coffee": "~0.7.0",
    "grunt-contrib-compress": "^0.8.0",
    "grunt-contrib-connect": "~0.5.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-jade": "~0.8.0",
    "grunt-contrib-stylus": "^0.21.0",
    "grunt-contrib-uglify": "~0.2.7",
    "grunt-contrib-yuidoc": "~0.5.0",
    "grunt-este-watch": "^0.1.16",
    "grunt-exec": "^0.4.6",
    "grunt-ngmin": "0.0.3",
    "grunt-release": "^0.9.0",
    "ibrik": "^2.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coffee-preprocessor": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.0",
    "load-grunt-tasks": "^0.4.0",
    "mocha": "~1.14.0",
    "sinon": "~1.9.1",
    "xo": "^0.16.0"
  },
  "author": "Yujiro Takeda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ujiro99/RedmineTimeTracker/issues"
  }
}
