.numbered-label
  color #555
  border: none
  padding: 0 3px 0 0
  background-color: transparent
  outline: none
  display: flex
  flex: 1

.numbered-label__number
  padding-right: 0.5em
  min-width: 2em
  text-align: right
  color #aaa

.numbered-label__label
  flex: 1
  text-align: left
  text-ellipsis()

.numbered-label__star
  font-size: 1em
  padding-right: 0.2em
